<div class="container px-13">
   <div class="space2">
      <h2><strong> Cuestionarios </strong></h2>
      <hr class="style17">
      <br>
      <div class="container">
         <div class="col-sm-12">
            <div class="accordion" id="accordionExample">
               <div class="accordion-item">
                  <h2 class="accordion-header" id="headingOne">
                     <button class="accordion-button" type="button" data-bs-toggle="collapse"
                        data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        <h5> Cuestionarios de Craft </h5>
                     </button>
                  </h2>
                  <div id="collapseOne" class="accordion-collapse collapse show" aria-labelledby="headingOne"
                     data-bs-parent="#accordionExample">
                     <div class="accordion-body">
                        <table class="table" style="table-layout: fixed">
                           <thead class="thead-dark">
                              <tr>
                                 <th style="text-align:center" scope="col">Nombre</th>
                                 <th style="text-align:center" scope="col"> Nivel </th>
                                 <th style="text-align:center" scope="col"> Fecha de modificación </th>
                              </tr>
                           </thead>
                           <tbody>
                              <% for (let cuestionario of cuestionarios) { %>
                              <% if (cuestionario.idDimension == 1 ) { %>
                              <tr>
                                 <td>
                                    <%=cuestionario.nombre%>
                                 </td>
                                 <td>
                                    <%=cuestionario.nivelC%>
                                 </td>
                                 <td>
                                    <% var c=cuestionario.fecha; %>
                                    <%= new Date(c).toISOString().slice(0, 19).replace('T', ' ' ); %>

                                 </td>
                                 <td>
                                    <% var url2='/feedback/verCuestionario/' + cuestionario.idCuestionario+'/' + cuestionario.nombre;%>
                                    <a href="<%=url2%>" type="button" style="margin:auto"
                                       class="btn btn-outline-dark col-xs-6" id="Ver_Cuest_Craft">
                                       Ver
                                    </a>

                                    <% var url='/feedback/editarCuestionario/' + cuestionario.idCuestionario + '/' + cuestionario.nivelC + '/' + cuestionario.idDimension + '/' + cuestionario.nombre;%>
                                    <a href="<%=url%>" type="button" style="margin:auto" class="btn btn-dark"
                                       id="Editar_Cuest_Craft">
                                       Editar <i class="bi bi-pencil-square"></i>
                                    </a>
                                 </td>
                              </tr>
                              <% } %>
                              <% } %>
                           </tbody>
                        </table>
                     </div>
                  </div>
               </div>
            </div>

            <br>

