<div class="container px-13">
   <div class="space2">
      <h2><strong> Cuestionarios </strong></h2>
      <hr class="style17">
      <br>
      <div class="container">
         <div class="col-sm-12">
            <button type="button" style="margin:auto" class="btn btn-dark"> + Nuevo Cuestionario</button>
            <br>
            <br>
            <div class="card">
               <h5 class="card-header"><i class="bi bi-file-earmark-text"></i> Cuestionarios</h5>
               <div class="card-body">
                  <!--Para tabla con boostrap-->
                  <table class="table" style="table-layout: fixed">
                     <thead class="thead-dark">
                        <tr>
                           <th style="text-align:center" scope="col">Nombre</th>
                           <th style="text-align:center" scope="col"> Dimensión </th>
                           <th style="text-align:center" scope="col"> Nivel </th>
                           <th style="text-align:center" scope="col"> Fecha de creación </th>
                        </tr>
                     </thead>
                     <tbody>
                        <% for (let cuestionario of cuestionarios) { %>
                           <tr>
                              <td>
                                 <%=cuestionario.nombre%>
                              </td>
                              <td>
                                 <%=cuestionario.idDimension%>
                              </td>
                              <td>
                                 <%=cuestionario.nivelP%>
                              </td>
                              <td>
                                 <% var c = cuestionario.fecha; %>
                                 <%= new Date(c).toISOString().slice(0, 19).replace('T', ' '); %>

                              </td>
                              <td>
                                 <button type="button" style="margin:auto" class="btn btn-outline-dark"><!--i class="bi bi-eye-fill"></i--> Ver </button>
                                 <button type="button" style="margin:auto" class="btn btn-dark"><!--i class="bi bi-pencil-square"></i--> Editar </button>
                              </td>
                           </tr>
                        <% } %>
                     </tbody>
                  </table>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>
